
# AWS
Host github.audible.com
  ProxyCommand=wssh proxy %h --port=%p

Host git.amazon.com
  ProxyCommand=wssh proxy %h --port=%p
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes

Host devdesk
  Hostname dev-dsk-brajit-1b-106566dd.eu-west-1.amazon.com
  IdentityFile ~/.ssh/id_rsa
  LogLevel ERROR # https://unix.stackexchange.com/questions/210211/disable-ssh-tunnel-open-failed-messages
  
# The following config is added by WSSH setup script to configure off-VPN dev-dsk access.
  ProxyCommand=wssh proxy %h
  ServerAliveInterval 15
  ServerAliveCountMax 44

  ForwardAgent yes
  LocalForward 8000 localhost:8000
  LocalForward 8080 localhost:8080
  LocalForward 8081 localhost:8081
  LocalForward 8082 localhost:8082
  LocalForward 8083 localhost:8083
  LocalForward 8443 localhost:8443
  LocalForward 2000 localhost:2000
  LocalForward 5005 localhost:5005
  LocalForward 5006 localhost:5006
  LocalForward 5007 localhost:5007
  LocalForward 5008 localhost:5008
  LocalForward 6000 localhost:6000
  LocalForward 6001 localhost:6001
  LocalForward 6002 localhost:6002
  LocalForward 6003 localhost:6003
  LocalForward 6379 localhost:6379
  LocalForward 7000 localhost:7000
  LocalForward 9229 localhost:9229
  LocalForward 3000 localhost:3000
  LocalForward 3001 localhost:3001
