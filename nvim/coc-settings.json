{
  "coc.preferences.messageLevel": "error",
  "cSpell.enabled": false,
  "cSpell.language": "en-GB",
  "explorer.file.reveal.auto": true,
  "explorer.file.root.template": "[icon] Project [hidden & 1][root]",
  "explorer.focus": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.keyMappings.global": {
    "*": "toggleSelection",
    "<2-LeftMouse>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "<<": "gitStage",
    "<cr>": ["wait", "expandable?", "cd", "open"],
    "<esc>": "esc",
    "<tab>": false,
    ">>": "gitUnstage",
    "A": "addDirectory",
    "R": "rename",
    "dd": "cutFile",
    "df": "delete",
    "e": "open",
    "f": "searchRecursive",
    "g<dot>": "toggleHidden",
    "i": false,
    "l": ["wait", "expandable?", "expand", "open"],
    "m": "actionMenu",
    "o": ["wait", "expanded?", "collapse", "expand"],
    "p": "pasteFile",
    "q": "quit",
    "r": "refresh",
    "s": "open:vsplit",
    "t": false,
    "u": ["wait", "gotoParent"],
    "yn": "copyFilename",
    "yp": "copyFilepath",
    "yy": "copyFile"
  },
  "explorer.mouseMode": "singleclick",
  "explorer.previewAction.onHover": "labeling",
  "explorer.sources": [
    {
      "expand": true,
      "name": "file"
    }
  ],
  "explorer.width": 27,
  "java.autobuild.enabled": false,
  "java.jdt.ls.vmargs": "-javaagent:/Users/rajitbanerjee/.local/lib/lombok.jar",
  "typescript.updateImportsOnFileMove.enable": true
}
