# system
alias rb="cd /mnt/c/Users/Rajit\ Banerjee"
alias ex="explorer.exe ."
alias sb="source ~/.bashrc"
alias open="xdg-open"
alias and="&&"

# git
alias ga="git add ."
alias gc="git commit -m"
alias gs="git status"
alias gp="git push"
alias gd="git diff"
alias gac="git add . && git commit -m"

# conda
alias cab="conda activate base"
alias cda="conda deactivate"
